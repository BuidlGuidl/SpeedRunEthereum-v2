{
  "name": "@se-2/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && sh -c 'if [ \"$VERCEL_ENV\" = \"production\" ]; then yarn drizzle-kit migrate; fi'",
    "check-types": "tsc --noEmit --incremental",
    "db:reset": "tsx services/database/scripts/reset-db.ts && yarn drizzle-kit migrate && yarn db:seed",
    "db:seed": "tsx services/database/seed.ts",
    "dev": "next dev",
    "drizzle-kit": "drizzle-kit",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "test": "playwright test",
    "test-ci": "playwright test --reporter=github",
    "test-smoke": "playwright test basic.spec.ts --reporter=list",
    "test-debug": "playwright test --debug",
    "test-cache": "yarn exec synpress",
    "test-ui": "playwright test --ui",
    "vercel": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:yolo": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1"
  },
  "dependencies": {
    "@heroicons/react": "~2.1.5",
    "@neondatabase/serverless": "^1.0.0",
    "@rainbow-me/rainbowkit": "^2.2.4",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.5.0",
    "@tanstack/react-query": "~5.59.15",
    "@tanstack/react-table": "^8.21.2",
    "@tanstack/react-virtual": "^3.13.2",
    "@uniswap/sdk-core": "~5.8.2",
    "@uniswap/v2-sdk": "~4.6.1",
    "blo": "~1.2.0",
    "burner-connector": "0.0.9",
    "daisyui": "4.12.24",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.42.0",
    "firebase-admin": "^13.4.0",
    "mime-types": "^3.0.1",
    "next": "~15.4.5",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "next-nprogress-bar": "~2.3.13",
    "next-plausible": "^3.12.4",
    "next-themes": "~0.3.0",
    "pg": "^8.13.1",
    "qrcode.react": "~4.0.1",
    "react": "~19.1.1",
    "react-copy-to-clipboard": "~5.1.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "~19.1.1",
    "react-dropzone": "^14.3.8",
    "react-flags-select": "^2.4.0",
    "react-hot-toast": "~2.4.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "usehooks-ts": "~3.1.0",
    "viem": "2.21.32",
    "wagmi": "2.12.23",
    "zustand": "~5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@synthetixio/synpress": "4.0.10",
    "@tailwindcss/typography": "^0.5.16",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "~22.17.0",
    "@types/pg": "^8",
    "@types/react": "~19.1.9",
    "@typescript-eslint/eslint-plugin": "~5.40.0",
    "abitype": "1.0.6",
    "autoprefixer": "~10.4.20",
    "csv-parse": "^5.6.0",
    "drizzle-kit": "^0.30.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "~8.57.1",
    "eslint-config-next": "~15.4.5",
    "eslint-config-prettier": "~8.10.0",
    "eslint-plugin-prettier": "~5.2.1",
    "postcss": "~8.4.45",
    "prettier": "~3.3.3",
    "tailwindcss": "~3.4.11",
    "tsx": "^4.19.2",
    "type-fest": "~4.26.1",
    "typescript": "<5.6.0",
    "vercel": "~39.1.3"
  },
  "packageManager": "yarn@3.2.3"
}
